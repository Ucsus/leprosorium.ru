	<div id="js-chat_resizer" class="chat_resizer"></div>
	<div class="b-chat_open_holder">
	    <a href="#" class="b-chat_open" onclick="chatHandler.close(); return false;"></a>
	</div>
	<a href="#" class="b-chat_close" onclick="chatHandler.close(); return false;">×</a>
</div>
